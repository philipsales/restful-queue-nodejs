{
  "name": "graphql-apollo-federated",
  "version": "1.0.0",
  "description": "Graphql Apollo boiler-plate",
  "main": "apollo.config.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"npm run \" \"command2 arg\"",
    "domains": "NODE_ENV=development concurrently \"nodemon src/domains/messages/index.js \"  \"nodemon src/domains/residents/index.js\" ",
    "gateway": "NODE_ENV=development nodemon gateway.js",
    "message": "NODE_ENV=development nodemon src/domains/messages/index.js",
    "resident": "NODE_ENV=development nodemon src/domains/residents/index.js",
    "serve": "npm run build && nodemon src/index.js",
    "start": "node src/index.js",
    "deploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipsales/graphql-express.git"
  },
  "keywords": [
    "graphql",
    "apollo"
  ],
  "author": "Philip Sales",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philipsales/graphql-express/issues"
  },
  "homepage": "https://github.com/philipsales/graphql-express#readme",
  "devDependencies": {
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@apollo/gateway": "^0.10.0",
    "apollo-datasource-rest": "^0.6.1",
    "apollo-server": "^2.8.2",
    "app-module-path": "^2.2.0",
    "axios": "^0.19.0",
    "concurrently": "^4.1.2",
    "couchbase": "^2.5.1",
    "fs": "0.0.1-security",
    "graphql": "^14.5.0",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-tool": "^1.0.0",
    "lodash": "^4.17.15",
    "merge-files": "^0.1.2",
    "merge-graphql-schemas": "^1.7.0",
    "node-fetch": "^2.6.0",
    "npm": "^6.11.2",
    "path": "^0.12.7"
  }
}
